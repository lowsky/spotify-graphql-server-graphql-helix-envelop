{
  "name": "spot-graphql-server",
  "version": "0.4.0",
  "private": true,
  "engines": {
    "yarn": ">=1.22.17",
    "node": ">=18.x"
  },
  "scripts": {
    "test": "npm run simpletest",
    "build": "sass public/stylesheets/style.sass:public/stylesheets/style.css",
    "simpletest": "tsx test/schemaTest.ts",
    "printSchema": "tsx bin/printSchema.ts",
    "watch": "npm run build ; tsx watch -r dotenv/config serverInit.ts",
    "start": "npm run build ; tsx -r dotenv/config serverInit.ts",
    "prepare": "yarn run snyk-protect",
    "snyk-protect": "snyk-protect"
  },
  "dependencies": {
    "@envelop/core": "5.4.0",
    "@graphql-tools/mock": "9.1.4",
    "@snyk/protect": "1.1301.1",
    "@whatwg-node/fetch": "0.10.13",
    "cors": "2.8.5",
    "dotenv": "17.2.3",
    "express": "5.2.1",
    "graphql": "16.12.0",
    "graphql-helix": "1.13.0",
    "sass": "1.95.1",
    "typescript": "5.9.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.19",
    "@types/express": "5.0.6",
    "@types/node": "24.10.2",
    "@vercel/node": "5.5.15",
    "tsx": "4.21.0"
  },
  "snyk": true
}
